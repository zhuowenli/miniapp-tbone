{
	"name": "vue-improve-loader",
	"version": "0.3.5",
	"description": "a tool for develop miniapp with vue",
	"main": "src/index.js",
	"miniapp": "src",
	"scripts": {
		"test": "jest --bail",
		"test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --bail",
		"coverage": "jest --coverage --bail"
	},
	"jest": {
		"testEnvironment": "node",
		"testMatch": [
			"**/test/**/*.test.js"
		],
		"collectCoverageFrom": [
			"src/**/*.js"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wechat-miniapp/tbone.git"
	},
	"author": "wechat-miniapp",
	"license": "MIT",
	"devDependencies": {
		"jest": "^24.8.0"
	}
}
